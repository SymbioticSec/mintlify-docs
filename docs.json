{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "maple",
  "name": "Symbiotic Security",
  "colors": {
    "primary": "#C45D97",
    "light": "#C45D97",
    "dark": "#C45D97"
  },
  "favicon": "images/favicon_symbiotic.png",
  "icons": {
    "library": "lucide"
  },
  "interaction": {
    "drilldown": true
  },
  "navigation": {
    "global": {
    "anchors":[
        {
          "anchor": "Website",
          "href": "https://symbioticsec.ai",
          "icon": "globe"
        },
        {
          "anchor": "Portal",
          "href": "https://app.symbioticsec.ai",
          "icon": "app-window"
        },
        {
          "anchor": "Changelog",
          "href": "https://symbioticsecurity.mintlify.app/changelog",
          "icon": "logs"
        }
      ]},
    "dropdowns": [
      {
        "dropdown": "Devs",
        "icon": "binary",
        "groups": [
          {
            "group": "Getting started",
            "pages": [
              "content/devs/intro/intro",
              "content/devs/intro/quickstart"
            ]
          },
          {
            "group": "Tools",
            "pages": [
              {
                "group": "IDE extension",
                "pages": [
                  "content/devs/discover_extension/detection",
                  "content/devs/discover_extension/treeview",
                  "content/devs/discover_extension/details",
                  "content/devs/discover_extension/hover",
                  "content/devs/discover_extension/status"
                  
                ]
              },
            "content/devs/discover_extension/MCP",
            "content/devs/discover_extension/CLI"
            ]
          },
          {
            "group": "Configurations",
            "pages": [
              "content/devs/actions/configure_detection",
              "content/devs/actions/ignore",
              "content/devs/actions/false_positive"
            ]
          },
          {
            "group": "Support",
            "pages": [
              "content/devs/support/share_logs",
              "content/devs/support/support_bundle",
              "content/devs/support/common_problems",
              "content/devs/support/commands"
            ]
          }
        ]
      },
      {
        "dropdown": "Admins",
        "icon": "user-cog",
        "groups": [
          {
            "group": "Admin features",
            "pages": [
              "content/admins/user_management",
              "content/admins/webhooks",
              "content/admins/guardrails"
            ]
          },
          {
            "group": "PR & CI",
            "pages": [
              "content/admins/vcs_integration",
              "content/admins/github_integration",
              "content/admins/setup_CI_integration"
            ]
      }
    ]
    }]
  },
  "logo": {
    "light": "images/logo/logo horizontal_black.svg",
    "dark": "images/logo/logo horizontal_white.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:support@symbioticsec.ai"
      },
      {
        "label": "Status",
        "href": "https://status.symbioticsec.ai/"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Download extension",
      "href": "https://symbioticsec-production-vscode-plugin-builds.s3.us-west-2.amazonaws.com/latest/symbioticsec.vsix"
    }
  },
  "contextual": {
    "options": [
      "copy",
      "view",
      "chatgpt",
      "claude",
      "perplexity",
      "mcp",
      "cursor",
      "vscode"
    ]
  },
  "footer": {
    "socials": {
      "linkedin": "https://linkedin.com/company/symbiotic-security"
    }
  }
}

